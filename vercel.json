{
    "version": 2,
   "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/ecommerce_frontend"
      }
    }
  ],
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/index.html"
        }
      ],
    "cleanUrls": true,
    "trailingSlash": false
  }
  