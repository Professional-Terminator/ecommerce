{
    "version": 2,
    "builds": [
      {
        "src": "angular.json",
        "use": "@vercel/angular"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      }
    ],
    "outputDirectory": "dist/ecommerce_frontend",
    "cleanUrls": true,
    "trailingSlash": false
  }
  