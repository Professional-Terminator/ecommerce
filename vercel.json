{
    "version": 2,
    "builds": [
      {
        "src": "angular.json",
        "use": "@vercel/angular",
        "config": {
          "distDir": "dist/your-angular-app"
        }
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "outputDirectory": "dist/ecommerce_frontend",
    "buildCommand": "ng build",
    "cleanUrls": true,
    "trailingSlash": false
  }
  